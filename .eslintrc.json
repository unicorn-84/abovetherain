{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "parserOptions": {
    "allowImportExportEverywhere": true
  },
  "rules": {
    "import/no-extraneous-dependencies": [2, {
      "devDependencies": true
    }],
    "linebreak-style": [
      "error",
      "windows"
    ]
  },
  "env": {
    "browser": true
  }
}
