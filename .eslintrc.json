{
  "extends": "airbnb-base",
  "parserOptions": {
    "allowImportExportEverywhere": true
  },
  "rules": {
    "linebreak-style": [
      "error",
      "windows"
    ],
    "import/no-unresolved": [
      "error",
      {
        "ignore": ["bootstrap.scss", "style.scss", "carousel", "collapse", "select"]
      }
    ],
    "max-len": 0
  },
  "env": {
    "browser": true,
    "jquery": true,
    "node": true,
    "es6": true
  }
}
