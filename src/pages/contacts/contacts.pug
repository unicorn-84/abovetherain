extends ../../templates/layout
append variables
  -const { options, pages, primaryNavigation } = require('../../data');
//-HEAD
append meta
  //Page meta tags
  title= `${options.siteName} | ${pages.contacts.title}`
  meta(name="description" content= pages.description)
append icons
  //Page icons
append styles
  //Page styles
//-BODY
append header
  +header(primaryNavigation.items)
append content
  +pageTitle(pages.contacts)
  section#content
    .content-wrap
      .container.clearfix
        .row
          .col-lg-12
            .address-item
              p
                i.icon-map-marker
                |Пионерская ул., 50
              p
                i.icon-phone3
                a(href="tel:+7812100-20-30") +7 812 100-20-30
              p
                i.icon-envelope-alt
                //-TODO
                  data-ajax
                a(href="#") info@admin.ru
          .col-lg-12
            #google-map
append scripts
  //Page scripts
  script(src="https://maps.google.com/maps/api/js?key=AIzaSyDMxJ92oBkSnVNHFX3R8XhtYQPEgk1_IiI")
  script(src="scripts/jquery.gmap.js")
  script.
    $('#google-map').gMap({
    latitude: 59.964123,
    longitude:  30.273157,
    maptype: 'ROADMAP',
    zoom: 15,
    markers: [
    {
    latitude: 59.964123,
    longitude:  30.273157,
    html: '<div style="width: 200px;"><h4 class="nomargin">Привет, мы Танцевальный&nbsp;Клуб <span>\'Над&nbsp;Дождем\'</span></h4></div>',
    icon: {
    image: "../images/icons/map-icon-red.png",
    iconsize: [32, 39],
    iconanchor: [32,39]
    }
    }
    ],
    doubleclickzoom: false,
    controls: {
    panControl: true,
    zoomControl: true,
    mapTypeControl: true,
    scaleControl: false,
    streetViewControl: false,
    overviewMapControl: false
    }
    });
