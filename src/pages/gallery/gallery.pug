extends ../../templates/layout
append variables
  -const { common, options, pages, primaryNavigation } = require('../../data');
//-HEAD
append meta
  //Page meta tags
  title= `${common.name} | ${pages.gallery.title}`
  if pages.gallery.description
    meta(name='description', content= pages.gallery.description)
  else
    meta(name='description', content= common.description)
  if pages.gallery.keys
    meta(name='keywords', content= pages.gallery.keys)
append icons
  //Page icons
append styles
  //Page styles
//-BODY
append header
  +header(primaryNavigation.items)
append content
  +pageTitle(pages.gallery)
  section#content
    .content-wrap
      .container
        div(id='gallery', data-lightbox='gallery')
          div
            a(data-lightbox='gallery-item')
            img(src= require(`../../images/gallery/thumbnails/1.jpg`) alt= options.siteName width='', height='')
append scripts
  //Page scripts
