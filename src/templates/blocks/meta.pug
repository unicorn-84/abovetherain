//Common meta tags
- const { common } = require('../../data');
meta(charset='utf-8')
meta(http-equiv='x-ua-compatible', content='ie=edge')
meta(name='viewport', content='width=device-width, initial-scale=1, viewport-fit=cover')
meta(property='og:type', content='website')
meta(property='og:image', content= require('../../images/icons/og-image.png'))
meta(property='og:image:width', content='520')
meta(property='og:image:height', content='260')
meta(property='og:url', content= common.url)
meta(property='og:description', content= common.description)
meta(property='og:title', content= common.name)
link(rel='author', href='humans.txt')
