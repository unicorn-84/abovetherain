include ../mixins/primary-navigation
//Header
header#header.transparent-header.no-sticky
  #header-wrap
    .container.clearfix
      #primary-menu-trigger: i.icon-reorder
      //Logo
      #logo
        a(href="index.html" class="standard-logo" data-dark-logo=require("../../images/logo/logo-dark.png"))
          img(src=require("../../images/logo/logo.png") alt="Логотип танцевального клуба 'Над дождем'")
        a(href="index.html" class="retina-logo" data-dark-logo=require("../../images/logo/logo-dark@2x.png"))
          img(src=require("../../images/logo/logo-dark@2x.png") alt="Логотип танцевального клуба 'Над дождем'")
      //Primary Navigation
      +primary-navigation(items)